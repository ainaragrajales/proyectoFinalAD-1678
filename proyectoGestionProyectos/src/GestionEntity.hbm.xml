<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ainara.GestionEntity" table="gestion" schema="gestionProyectos">
        <id name="idGestion">
            <column name="id_gestion" sql-type="int unsigned"/>
        </id>
        <property name="cantidad">
            <column name="cantidad" sql-type="double" precision="-1" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>